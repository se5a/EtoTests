<?xml version="1.0" encoding="UTF-8"?>
<Panel
  xmlns="http://schema.picoe.ca/eto.forms" 
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  >
  <TableLayout>
    <TableRow>
      <ComboBox x:Name="CB" DataContext="{Binding ComboList}"/>
      <DropDown x:Name="DD" DataContext="{Binding DropList}"/>
      <TableCell/>
    </TableRow>
    <ListBox x:Name="LB" DataContext="{Binding BoxList}"/>    
  </TableLayout>
</Panel>